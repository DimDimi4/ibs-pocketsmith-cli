{
    "name": "ibs-pocketsmith",
    "description": "Israeli bank scrappers to PocketSmith",
    "version": "0.0.0",
    "author": "Dima",
    "bin": {
        "ibs-ps": "./bin/run"
    },
    "bugs": "https://github.com/dima/ibs-pocketsmith/issues",
    "dependencies": {
        "@oclif/command": "^1.8.20",
        "@oclif/config": "^1.18.6",
        "@oclif/plugin-help": "^5.1.20",
        "axios": "^1.2.1",
        "cli-progress": "^3.11.2",
        "conf": "^10.2.0",
        "date-fns": "^2.29.3",
        "date-fns-tz": "^1.3.7",
        "debug": "^4.3.4",
        "israeli-bank-scrapers": "^3.2.0",
        "node-json-db": "^2.1.3",
        "prompts": "^2.4.2"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.10",
        "@types/cli-progress": "^3.11.0",
        "@types/fs-extra": "^9.0.13",
        "@types/inquirer": "^9.0.3",
        "@types/node": "^18.11.13",
        "@types/prompts": "^2.4.2",
        "eslint": "^8.29.0",
        "eslint-config-oclif": "^4.0.0",
        "eslint-config-oclif-typescript": "^1.0.3",
        "nyc": "^15.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/dima/ibs-pocketsmith",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "ibs-ps",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "dima/ibs-pocketsmith",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "echo NO TESTS",
        "version": "oclif-dev readme && git add README.md"
    },
    "types": "lib/index.d.ts"
}
